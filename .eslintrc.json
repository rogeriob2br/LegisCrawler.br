{
  "extends": "airbnb-base",
  "rules":
  {
    "max-len": [
      "error",
      100,
      {
        "ignoreRegExpLiterals": true,
        "ignoreStrings": true
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true,
        "allow": ["_id"]
      }
    ]
  }
}
